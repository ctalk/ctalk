/*
 *    Collection class.
 */

Collection class Stream
"Stream class is the superclass of TerminalStream and
FileStream objects.";


Stream instanceMethod ++ incKeyRefPrefix (__prefix__) {
  _warning ("The ++ method is not (yet) implemented for Stream class and its subclasses.\n");
  return self;
}

Stream instanceMethod -- decKeyRefPrefix (__prefix__) {
  _warning ("The -- method is not (yet) implemented for Stream class and its subclasses.\n");
  return self;
}

Stream instanceMethod ++ incKeyRef (void) {
  _warning ("The ++ method is not (yet) implemented for Stream class and its subclasses.\n");
  return self;
}

Stream instanceMethod -- decKeyRef (void) {
  _warning ("The -- method is not (yet) implemented for Stream class and its subclasses.\n");
  return self;
}

Stream instanceMethod - subtractRef (Integer iArg) {
  _warning ("The - method is not (yet) implemented for Stream class and its subclasses.\n");
  return self;
}

Stream instanceMethod + addRef (Integer iArg) {
  _warning ("The + method is not (yet) implemented for Stream class and its subclasses.\n");
  return self;
}

Stream instanceMethod -= concatRcvrDec (Integer iArg) {
  _warning ("The -= method is not (yet) implemented for Stream class and its subclasses.\n");
  return self;
}

Stream instanceMethod += concatRcvr (Integer iArg) {
  _warning ("The += method is not (yet) implemented for Stream class and its subclasses.\n");
    return self;
}
